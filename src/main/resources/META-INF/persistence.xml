		<class>com.app.tennis.data.Match</class>
		<class>com.app.tennis.data.Arbitre</class>
		<class>com.app.tennis.data.Court</class>
		<class>com.app.tennis.data.NiveauArbitre</class>
		<class>com.app.tennis.data.TypeQualification</class>
		<class>com.app.tennis.data.Joueur</class>
		<class>com.app.tennis.data.Acces</class>
		
	</persistence-unit>
	
</persistence>

